{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github-token",
    "GITHUB_OWNER": "cochranfilms",
    "GITHUB_REPO": "cross-creek"
  }
}
