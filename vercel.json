{
  "version": 2,
  "buildCommand": "echo 'Static site - no build required'",
  "outputDirectory": ".",
  "functions": {
    "api/test.js": {
      "maxDuration": 30
    },
    "api/upload-contract.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/admin-dashboard", "destination": "/admin-dashboard.html" },
    { "source": "/user-portal", "destination": "/user-portal.html" },
    { "source": "/contract", "destination": "/contract.html" },
    { "source": "/admin", "destination": "/admin-dashboard.html" },
    { "source": "/backend", "destination": "/backend.html" },
    { "source": "/read-me", "destination": "/Read-Me.html" },
    { "source": "/Read-Me", "destination": "/Read-Me.html" },
    { "source": "/read-me.html", "destination": "/Read-Me.html" },
    { "source": "/Read-Me.html", "destination": "/Read-Me.html" },
    { "source": "/", "destination": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
